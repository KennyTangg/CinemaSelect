{
  "name": "mern_cinemaselect",
  "version": "1.0.0",
  "description": "A MERN project",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-deps": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "deploy": "npm run build && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KennyTangg/CinemaSelect.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KennyTangg/CinemaSelect/issues"
  },
  "homepage": "https://github.com/KennyTangg/CinemaSelect#readme",
  "dependencies": {
    "concurrently": "^8.2.2",
    "react-helmet": "^6.1.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-animations": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">=16.20.1"
  }
}
